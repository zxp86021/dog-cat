{
    "name": "zxp86021/the-dog-cat",
    "description": "The Unofficial SDK for TheCatAPI and TheDogAPI",
    "keywords": ["laravel", "TheCatAPI", "TheDogAPI"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "richegg Tsai",
            "email": "zxp86021+gh@gmail.com",
            "homepage": "https://richegg.tw"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^1.6",
        "illuminate/support": "^6 || ^7 || ^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "TheDogCat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheDogCat\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "TheDogCat\\Laravel\\TheDogCatServiceProvider"
            ],
            "aliases": {
                "TheDogCat": "TheDogCat\\Laravel\\Facades\\TheDogCat"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
